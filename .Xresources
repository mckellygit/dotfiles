    Ctrl Alt ~Shift   <Key> c: string("\033[2;5~") \n\
    Ctrl Shift ~Alt <Key> minus: smaller-vt-font() \n\
    Ctrl Shift ~Alt <Key> equal: larger-vt-font() \n\
    Ctrl Shift ~Alt <Key> 0: set-vt-font(d) \n\
    ! Alt <Btn1Down>: popup-menu(mainMenu) \n\
    ! Alt <Btn2Down>: popup-menu(vtMenu) \n\
    ! Alt <Btn3Down>: popup-menu(fontMenu) \n\
    ! Ctrl <Btn1Down>: ignore() \n\
    ! Ctrl <Btn2Down>: ignore() \n\
    ! Ctrl <Btn3Down>: ignore() \n\
    ! Shift <Btn1Down>: ignore() \n\
    ! Shift <Btn1Motion>: ignore() \n\
    ! Shift Button1<PtrMoved>: ignore() \n\
    ! Shift <Btn1Up>: ignore() \n\
    ! Shift <Btn3Down>: ignore() \n\
    Ctrl Shift ~Alt <Btn1Down>: select-start(CLIPBOARD) \n\
    Ctrl Shift ~Alt <Btn1Motion>: select-extend(CLIPBOARD) \n\
    Ctrl Shift ~Alt <Btn1Up>: select-end(CLIPBOARD) \n\
    Ctrl Shift ~Alt <Btn3Down>: start-extend(CLIPBOARD) \n\
    Ctrl Shift ~Alt <Btn3Motion>: select-extend(CLIPBOARD) \n\
    Ctrl Shift ~Alt <Btn3Up>: select-end(CLIPBOARD)

!! NOTE: MUST set this to get mouse selections to work inside tmux ...
