!! ==========================================
!! ==========================================

URxvt*termName: rxvt-unicode-256color

URxvt.font:     xft:DejaVu Sans Mono:pixelsize=21
URxvt.boldFont: xft:DejaVu Sans Mono:pixelsize=21:weight=bold

URxvt.letterSpace: 0
URxvt.lineSpace: 0
URxvt.geometry: 168x48+800+850
URxvt*externalBorder: 1
URxvt.internalBorder: 6
URxvt.borderColor: #262626
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.saveline: 0
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.urgentOnBell: true
URxvt.iso14755: false
URxvt.iso14755_52: false

*.foreground:  #ffd1b7
!!*.background:  #2e2e28
urxvt*depth: 32
!!urxvt*background: rgba:0000/0000/0200/c800
!!urxvt*background: rgba:0000/0000/0000/cccc
!!urxvt*background: rgba:3400/3400/3400/fa00
URxvt.background: [96]#2e2e28
!!urxvt*transparent: true
!!urxvt*shading: 30

URxvt.keysym.Home:              \033[1~
URxvt.keysym.End:               \033[4~
URxvt.keysym.Control-Up:        \033[1;5A
URxvt.keysym.Control-Down:      \033[1;5B
URxvt.keysym.Control-Left:      \033[1;5D
URxvt.keysym.Control-Right:     \033[1;5C

URxvt.keysym.C-S-Left:		    string:\033[1;6D
URxvt.keysym.C-S-Right:		    string:\033[1;6C
URxvt.keysym.C-S-Up:            string:\033[1;6A
URxvt.keysym.C-S-Down:          string:\033[1;6B

URxvt.keysym.C-BackSpace:       string:\033\177
URxvt.keysym.C-Insert:          string:\033\061
URxvt.keysym.C-S-BackSpace:     string:\033\062
URxvt.keysym.C-equal:           string:\033\063
URxvt.keysym.C-End:             string:\033\066
URxvt.keysym.C-Home:            string:\033\065
URxvt.keysym.C-S-space:         string:\033\064

!! ==========================================

URxvt.copyCommand:   xsel -i -p --rmlastnl --sc 0
URxvt.pasteCommand:  xsel -o -p --rmlastnl --sc 0

URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard

URxvt.keysym.Control-Meta-c: builtin-string:
URxvt.keysym.Control-Meta-v: builtin-string:

URxvt.keysym.Control-minus:   resize-font:smaller
URxvt.keysym.Control-plus:    resize-font:bigger

!! ==========================================
!! ==========================================

!! disable scrollback buffer for alternate screen
!! thus shift+up etc. does not scroll ...
URxvt.secondaryScreen: 1
URxvt.secondaryScroll: 0
URxvt.secondaryWheel:  1

!!no command ...
!!URxvt.keysym.C-BackSpace: command:\000

!! Common Keybinds for Navigations
!!URxvt.keysym.Shift-Up: command:\033]720;1\007
!!URxvt.keysym.Shift-Down: command:\033]721;1\007

!! Perl extensions
!!URxvt.perl-ext-common: default,matcher,font-size
!! Open urls in browser with Control-Click
!!URxvt.urlLauncher: /usr/bin/firefox
URxvt.matcher.button: C1
!! Change font size on Control-Plus/Minus
!!URxvt.keysym.Control-0x2b: perl:font-size:increase
!!URxvt.keysym.Control-0x2d: perl:font-size:decrease
!! Copy Paste & Other Extensions
!!URxvt.perl-ext-common: default,clipboard,url-select,keyboard-select
URxvt.perl-ext-common: default, matcher
!!URxvt.keysym.M-c: perl:clipboard:copy
!!URxvt.keysym.M-v: perl:clipboard:paste
!!URxvt.keysym.M-C-v: perl:clipboard:paste_escaped
!!URxvt.keysym.M-Escape: perl:keyboard-select:activate
!!URxvt.keysym.M-s: perl:keyboard-select:search
!!URxvt.keysym.M-u: perl:url-select:select_next
URxvt.urlLauncher: chrome
URxvt.underlineURLs: true
URxvt.urlButton: 1

URxvt.autocopy: false
URxvt.copycmd:  xsel -ib
URxvt.pastecmd: xsel -ob

