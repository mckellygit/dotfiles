URxvt.iso14755:                 false
URxvt.iso14755_52:              false

!! special ------
URxvt.keysym.S-BackSpace:       string:\177
URxvt.keysym.C-S-BackSpace:     string:\177

URxvt.keysym.C-S-space:         string:\033\044

URxvt.keysym.Shift-Control-X:   string:\033\050

URxvt.keysym.Control-Meta-BackSpace: string:\033\100
URxvt.keysym.Control-Meta-equal:     string:\033\043
URxvt.keysym.Control-Meta-space:     string:\033\045

URxvt.keysym.C-BackSpace:        command:\000
URxvt.keysym.C-equal:            command:\000
URxvt.keysym.C-minus:            command:\000
URxvt.keysym.Control-Meta-minus: command:\000
